package smallprogramsinterviewpreparation;

public class primenumber {

	public static void main(String[] args) {
		
		int n=17;
		int count=0;
		
		for(int i=2;i<=n;i++) {
			
			if(n%i==0) {
				System.out.println("numbere is not prime");
				
				break;
			}
		}
		
		if(count==0) {
			System.out.println("number is prime");
		}

	}

}
